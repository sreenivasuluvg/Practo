<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.studiocorp.practo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/practo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/practo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            >
            <intent-filter >
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>

        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
        <activity android:name=".Search" />
        <activity
            android:name=".AddDetails"
            android:label="@string/title_activity_add_details"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AddIntrest"
            android:label="@string/title_activity_add_intrest"
            android:theme="@style/AppTheme.NoActionBar" />

    </application>

</manifest>